# DSE 5.1 Operations Training - Environment
# Copy to .env and adjust for your machine.

# Container runtime: docker or podman (scripts use this to pick compose/exec commands)
CONTAINER_RUNTIME=docker

# DSE image (use a 5.1.x tag from Docker Hub, e.g. 5.1.25)
DSE_IMAGE=datastax/dse-server:5.1.49

# Cluster identity
CLUSTER_NAME=DSE
DC=DC1